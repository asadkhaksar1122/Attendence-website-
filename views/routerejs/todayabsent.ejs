<% layout('layout/boilerplate') -%>
<body>
    <h1 class="Attendence-heading">Today Absent</h1>
    <div class="main-table">
        <table class="table">
  <tr>
    <th>Name</th>
    <th>Username</th>
    <th>Date</th>
    <th>Attendence</th>
   
  </tr>
  <% for (const attendence of todayabsent) {%>
     <tr>
    <td><%= attendence.student.name %></td>
    <td><%= attendence.student.username %></td>
    <td><%= attendence.date %></td>
    <td><%= attendence.attendence %></td>
  </tr>
 <%  } %>
 
  
</table>
    </div>
</body>